{
  "name": "tanstack-router-simple",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "yarn run build.client && yarn run build.server",
    "build.client": "NODE_ENV=production vite build",
    "build.server": "NODE_ENV=production vite build --ssr",
    "start": "node dist/server.js --port 3000",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@hono/node-server": "^1.7.0",
    "@ssrx/plugin-tanstack-router": "latest",
    "@ssrx/react": "latest",
    "@ssrx/vite": "latest",
    "@tanstack/react-router": "1.32.0",
    "@tanstack/router-devtools": "1.32.0",
    "@tanstack/router-vite-plugin": "1.31.18",
    "hono": "3.12.10",
    "jsesc": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.2.51",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.17",
    "postcss": "8.4.33",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-tsconfig-paths": "4.3.1"
  }
}
