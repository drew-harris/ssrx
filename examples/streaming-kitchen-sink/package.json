{
  "name": "streaming-kitchen-sink",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "bun run --bun vite dev --port 3000",
    "build": "yarn run build.client && yarn run build.server",
    "build.client": "NODE_ENV=production vite build",
    "build.server": "NODE_ENV=production vite build --ssr",
    "start": "NODE_ENV=production node dist/server.js --port 3000",
    "start.bun": "NODE_ENV=production bun run dist/server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@hono/node-server": "1.2.0",
    "@super-ssr/plugin-react-router": "latest",
    "@super-ssr/plugin-tanstack-query": "latest",
    "@super-ssr/plugin-trpc-react": "latest",
    "@super-ssr/plugin-unhead": "latest",
    "@super-ssr/react": "latest",
    "@tanstack/react-query": "5.0.0-rc.12",
    "@tanstack/react-query-devtools": "5.0.0-rc.13",
    "@trpc/server": "10.40.0",
    "autoprefixer": "10.4.16",
    "hono": "3.7.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.16.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@super-ssr/vite": "latest",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@vitejs/plugin-react": "4.1.0",
    "vite": "5.0.0-beta.6",
    "vite-tsconfig-paths": "4.2.1"
  }
}
