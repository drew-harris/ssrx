{
  "name": "streaming-kitchen-sink",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "yarn run build.client && yarn run build.server",
    "build.client": "NODE_ENV=production vite build",
    "build.server": "NODE_ENV=production vite build --ssr",
    "start": "NODE_ENV=production node dist/server.js --port 3000",
    "start.bun": "NODE_ENV=production bun run dist/server.js",
    "typecheck": "tsc",
    "--db related commands below--": "",
    "db.push": "drizzle-kit push:sqlite",
    "migrate.up": "tsx src/server/db/migrate.ts",
    "migrate.generate": "drizzle-kit generate:sqlite",
    "migrate.check": "drizzle-kit check:sqlite",
    "debug": "tsx debug.ts"
  },
  "dependencies": {
    "@decs/typeschema": "0.12.1",
    "@hono/node-server": "1.2.0",
    "@hono/valibot-validator": "0.2.0",
    "@lucia-auth/adapter-sqlite": "2.0.0",
    "@paralleldrive/cuid2": "2.2.2",
    "@super-ssr/plugin-react-router": "latest",
    "@super-ssr/plugin-tanstack-query": "latest",
    "@super-ssr/plugin-trpc-react": "latest",
    "@super-ssr/plugin-unhead": "latest",
    "@super-ssr/react": "latest",
    "@tanstack/react-query": "5.0.0-rc.12",
    "@tanstack/react-query-devtools": "5.0.0-rc.13",
    "@trpc/server": "10.40.0",
    "better-sqlite3": "9.0.0",
    "drizzle-orm": "0.28.6",
    "drizzle-valibot": "0.1.1",
    "hono": "3.7.6",
    "lucia": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.16.0",
    "valibot": "0.19.0"
  },
  "devDependencies": {
    "@super-ssr/vite": "latest",
    "@types/better-sqlite3": "7.6.5",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@vitejs/plugin-react": "4.1.0",
    "autoprefixer": "10.4.16",
    "drizzle-kit": "0.19.13",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.3",
    "tsx": "3.13.0",
    "vite": "5.0.0-beta.6",
    "vite-tsconfig-paths": "4.2.1"
  }
}
