{
  "name": "@dete/vite",
  "version": "0.0.1",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "vite-plugin"
  ],
  "exports": {
    "./types": "./src/types.ts",
    "./plugin": {
      "bun": "./src/plugin-entry.ts",
      "import": "./dist/plugin-entry.js"
    },
    "./runtime": {
      "bun": "./src/runtime/index.ts",
      "import": "./dist/runtime/index.js"
    }
  },
  "scripts": {
    "dev": "yarn build --watch",
    "build": "tsup",
    "test": "vitest",
    "test.watch": "vitest watch --update",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "vite": ">=4"
  },
  "dependencies": {
    "radix3": "^1.1.0"
  },
  "devDependencies": {
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vite": "5.0.0-beta.4",
    "vitest": "0.34.6"
  }
}
